		// c&#243;digo javascript para permitir acessibilidade nos menus 
		window.addEventListener("load", function() {
			let focoAnterior = null;
			
			document.addEventListener('keyup', (e) => {
				if (e.keyCode == 9 && !e.shiftKey) { // pressionado tab para avan&#231;ar nos menus
					let menus = document.getElementsByClassName('rich-panelbar-interior');
				    for (let i = 0; i < menus.length - 1; i++) {
				        let submenus = menus[ i ].getElementsByClassName('itemMenu');
				        if (focoAnterior == submenus[ submenus.length - 1 ].parentElement) {
				            menus[ i + 1 ].getElementsByClassName('rich-panelbar-header-act')[ 0 ].click();
				            menus[ i + 1 ].getElementsByClassName('itemMenu')[ 0 ].parentElement.focus();
				            break;
				        }
				    }
				}
				else if (e.keyCode == 9 && e.shiftKey) { // pressionado shift + tab para voltar nos menus
					let menus = document.getElementsByClassName('rich-panelbar-interior');
				    for (let i = 1; i < menus.length; i++) {
				        let submenus = menus[ i ].getElementsByClassName('itemMenu');
				        if (focoAnterior == submenus[ 0 ].parentElement) {
				            menus[ i - 1 ].getElementsByClassName('rich-panelbar-header-act')[ 0 ].click();
				            let t = menus[ i - 1 ].getElementsByClassName('itemMenu');
				            t[ t.length - 1 ].parentElement.focus();
				            break;
				        }
				    }
				}  
			})
		
			// configura evento para salvar o foco anterior dos elementos
			let menus = document.getElementsByClassName('rich-panelbar-interior');
			for (let i = 0; i < menus.length; i++) {
				let submenus = menus[ i ].getElementsByClassName('itemMenu');
				for (let j = 0; j < submenus.length; j++) {
					submenus[ j ].parentElement.addEventListener("blur", (e) => {
						focoAnterior = e.target;
					})	
				}
			}
		})
		</script>
		

		
